{
	"manifest_version": 3,
	"name": "CM URL Magic",
	"version": "0.0.1",
	"description": "A simple extension to make your life easier",
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_title": "CM URL Magic",
		"default_popup": "index.html"
	},
	"permissions": ["activeTab", "storage"]
}
